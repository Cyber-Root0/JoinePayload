<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff3c3c3c" android:endColor="#ff3c3c3c" android:angle="0.0" android:centerY="0.5" android:centerColor="#ff3c3c3c" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffa2a2a2" android:endColor="#ffa2a2a2" android:angle="0.0" android:centerY="0.5" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff247fff" android:endColor="#ff247fff" android:angle="0.0" android:centerY="0.5" />
            </shape>
        </clip>
    </item>
</layer-list>