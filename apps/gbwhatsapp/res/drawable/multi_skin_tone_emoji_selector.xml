<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@drawable/emoji_selected_state" />
    <item android:state_focused="true" android:drawable="@drawable/emoji_selected_state" />
    <item android:state_selected="true" android:drawable="@drawable/emoji_selected_state" />
    <item android:drawable="@drawable/emoji_selected_state" android:state_activated="true" />
    <item>
        <shape>
            <solid android:color="@color/transparent" />
            <corners android:radius="@dimen/multi_skin_tone_selector_corner_radius" />
        </shape>
    </item>
</selector>