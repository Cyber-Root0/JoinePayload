<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/common_dimens_40dp" />
            <solid android:color="#ffe1e1e1" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/common_dimens_40dp" />
                <gradient android:startColor="@color/modulegame_color_ff6200" android:endColor="@color/modulegame_color_ff5a00" android:angle="0.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/common_dimens_40dp" />
                <gradient android:startColor="@color/modulegame_color_ff6200" android:endColor="@color/modulegame_color_ff5a00" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>